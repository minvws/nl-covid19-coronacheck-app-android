<resources>
    <declare-styleable name="AccessibilityHeaderWidget">
        <attr name="isAccessibilityHeader" format="boolean" />
    </declare-styleable>
    <declare-styleable name="HtmlTextViewWidget">
        <attr name="htmlText" format="string" />
        <attr name="htmlTextColor" format="color" />
        <attr name="htmlTextColorLink" format="color" />
        <attr name="enableHtmlLinks" format="boolean" />
        <attr name="textIsSelectable" format="boolean" />
        <attr name="listItemMarginMultiplier" format="float" />
        <attr name="headingMarginMultiplier" format="float" />
        <attr name="textSize" format="dimension" />
        <attr name="splitHtmlText" format="boolean" />
    </declare-styleable>
    <declare-styleable name="ScrollViewButtonWidget">
        <attr name="buttonText" format="string" />
        <attr name="secondaryButtonText" format="string" />
        <attr name="buttonEnabled" format="boolean" />
        <attr name="attachToScrollView" format="reference" />
    </declare-styleable>
    <declare-styleable name="ButtonWithProgressWidget">
        <attr name="text" format="string" />
    </declare-styleable>
    <declare-styleable name="ScrollViewCheckboxButtonWidget">
        <attr name="checkboxText" format="string" />
        <attr name="checkboxErrorText" format="string" />
        <attr name="checkboxButtonText" format="string" />
        <attr name="buttonHidden" format="boolean" />
        <attr name="attachCheckboxToScrollView" format="reference" />
        <attr name="src" format="reference|color" />
    </declare-styleable>
    <declare-styleable name="RecyclerViewButtonWidget">
        <attr name="attachToRecyclerView" format="reference" />
    </declare-styleable>
</resources>
